            <!--          IDLE MODE (0)                           -->
            <!--==================================================-->
            <entry value="0" name="ASV_RSGA_CUSTOM_MODE_IDLE">
                <description>Default mode. Do nothing.</description>
            </entry>
            <!--==================================================-->
            <!--          COMMON GENERATOR MODES (1-24)           -->
            <!--==================================================-->
            
            <!--==================================================-->
            <!--          COMMON ANALYZER MODES (25-49)           -->
            <!--==================================================-->
